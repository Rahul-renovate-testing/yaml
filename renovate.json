{
  "customManagers": [
    {
      "customType": "jsonata",
      "fileMatch": ["file.yaml"],
      "matchStrings": ["packages.{\"depName\": package, \"currentValue\": version}"],
      "autoReplaceStringTemplate": "- package: {{{depName}}}\n  version: {{{newValue}}}",
      "datasourceTemplate": "npm"
    }
  ]
}
